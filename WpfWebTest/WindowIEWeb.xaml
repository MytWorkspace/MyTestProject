<Window x:Class="WpfWebTest.WindowIEWeb"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfWebTest"
        mc:Ignorable="d"
        Title="WindowIEWeb" Height="450" Width="800">
    <Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="30" />
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>
		<StackPanel Orientation="Horizontal" Grid.Row="0" >
			<TextBox Width="500" Height="30" x:Name="txtBox" FontSize="20" />
			<Button Content="转到" Background="Aqua" Width="100" BorderBrush="Transparent" Margin="2,0,0,0" Click="Button_Click" />

			<Button Content="点击获取文本框" Click="Button_Click_1" />
		</StackPanel>
		<Grid x:Name="grid"  Grid.Row="1"  ></Grid>
	</Grid>
</Window>
